generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model secretTable {
  secret_id String @id @unique
  value     String @unique
}

model AnonymousUser {
  uuid                     String  @unique
  password                 String
  token                    String
  lastConnection           Int
  userCreatedAt            String
  revokeConnectionRequest  Boolean
  isRegistredAsRegularUser Boolean
}
