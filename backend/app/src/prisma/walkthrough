docker run -d -e POSTGRES_DB=db_postgres --name postgres -e POSTGRES_PASSWORD=password_postgres -e POSTGRES_USER=user_postgres -p 6500:5432 postgres

// des problemes de droits sont apparu et set chmod 777 .env schema.prisma
// modification de DATABASE_URL dans  prisma/.env adresse recupere a partir de docker inspect postgres


// on entre dans le backend container 

